{
  "name": "styrerommet-web",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development node -r babel-register src/server",
    "build": "babel src --out-dir build && next build src",
    "start": "NODE_ENV=production node build/server",
    "test": "jest",
    "test:ci": "npm run lint && npm run test && npm run format:check",
    "lint": "eslint src/**/*.js",
    "format:check": "prettier -l src/**/*.js",
    "format": "prettier --write src/**/*.js",
    "storybook": "STORYBOOK_USE_MOCK_DATE=$CI start-storybook -c src/storybook -s public -p ${PORT:=8088}",
    "storybook:snapshot": "CHROMATIC_APP_CODE=uyzl2y4bel PORT=8089 chromatic test --exit-zero-on-changes --storybook-port=8089",
    "precommit": "lint-staged"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "testRegex": "\\.test\\.js$",
    "testURL": "http://localhost"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --max-warnings 0",
      "prettier -l"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "semi": false,
    "printWidth": 100
  },
  "prettierMessage": "\\nUse npm run format:staged to fix the above files.",
  "dependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-export-default-from": "7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "@zeit/next-bundle-analyzer": "0.1.2",
    "@zeit/next-source-maps": "0.0.3",
    "apollo-cache-inmemory": "1.2.10",
    "apollo-client": "2.4.2",
    "apollo-link": "1.2.3",
    "apollo-link-error": "1.1.1",
    "apollo-link-http": "1.5.5",
    "apollo-link-state": "0.4.2",
    "autosize-input": "1.0.2",
    "browserslist-useragent": "2.0.1",
    "classnames": "2.2.6",
    "css-ease": "0.0.1",
    "date-fns": "2.0.0-alpha.7",
    "dotenv": "4.0.0",
    "emotion": "8.0.12",
    "emotion-server": "8.0.12",
    "express": "4.16.3",
    "express-basic-auth": "1.1.5",
    "focus-trap-react": "4.0.1",
    "force-ssl-heroku": "1.0.2",
    "graphql": "0.11.7",
    "graphql-tag": "2.10.0",
    "html-pdf": "2.2.0",
    "http-proxy-middleware": "0.19.0",
    "invariant": "2.2.4",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.17.11",
    "next": "7.0.1",
    "next-routes": "1.4.2",
    "polished": "1.9.2",
    "prop-types": "15.6.2",
    "raf": "3.4.0",
    "raven-js": "3.27.0",
    "react": "16.5.2",
    "react-adopt": "0.6.0",
    "react-animate-height": "2.0.5",
    "react-apollo": "2.2.4",
    "react-aria-modal": "3.0.0",
    "react-autosize-textarea": "3.0.3",
    "react-click-outside": "github:tj/react-click-outside",
    "react-content-loader": "3.1.2",
    "react-day-picker": "7.2.4",
    "react-dom": "16.3.2",
    "react-maskedinput": "4.0.1",
    "react-powerplug": "1.0.0-rc.1",
    "react-select": "1.2.1",
    "react-spring": "^5.9.2",
    "request": "2.88.0",
    "scroll-into-view-if-needed": "1.5.0",
    "scrollmonitor": "1.2.4",
    "superagent": "3.8.3",
    "universal-cookie": "2.1.2",
    "uuid": "3.3.2",
    "webpack": "4.20.2"
  },
  "devDependencies": {
    "@storybook/addon-options": "4.0.0-alpha.21",
    "@storybook/react": "4.0.0-alpha.21",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.2.6",
    "babel-plugin-emotion": "8.0.12",
    "chokidar": "1.7.0",
    "eslint": "4.19.1",
    "eslint-config-react-app": "2.1.0",
    "eslint-plugin-flowtype": "2.50.3",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.11.1",
    "jest": "23.6.0",
    "jest-date-mock": "1.0.5",
    "jest-dom": "1.3.1",
    "lint-staged": "6.1.1",
    "mockdate": "2.0.2",
    "prettier": "1.14.3",
    "react-testing-library": "3.1.7",
    "run4staged": "1.2.1",
    "staged-git-files": "1.1.1",
    "storybook-addon-apollo-graphql": "0.1.2",
    "storybook-chromatic": "1.0.2"
  }
}
